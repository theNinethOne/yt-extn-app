{
    "manifest_version": 3,
    "name": "yt-proto-one",
    "version": "1.0.0",
  
    "permissions": ["scripting", "activeTab", "storage", "tabs", "windows"],
  
    "action": {
      "default_popup": "index.html"
    },
  
    "host_permissions": ["<all_urls>"],
  
    "background" : {
          "service_worker" : "background.js",
          "type" : "module"
      },
  
      "content_scripts" : [
          {"matches" : ["<all_urls>"],
           "js" : ["script.js"]
          }
      ]
  }